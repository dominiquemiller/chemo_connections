

<h1>Listing Photos</h1>
<%= link_to 'New Photo', new_photo_path %>


<div class="container">
<% @photos.in_groups_of(4) do |group| %>
  <div class="row">
    <% group.compact.each do |photo| %>
      <div class="col-md-3">
        <%= link_to image_tag(photo.picture, :size => '200x200',class:"img-responsive"), photo%>
        <!-- <h4><%= link_to photo.title, photo %></h4>
        <%= photo.description %> -->
      </div>
    <% end %>
  </div>
<% end %>
</div>
