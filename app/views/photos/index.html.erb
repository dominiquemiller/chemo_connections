
<div class="row">
  <h1 class="text-center">Your memories and favorite moments.</h1>
</div>
  <h4><%= link_to 'New Photo', new_photo_path %></h4>


  <div class="container">
  <% @photos.in_groups_of(4) do |group| %>
    <div class="row">
      <% group.compact.each do |photo| %>
        <div class="col-md-3">
          <%= link_to image_tag(photo.picture, :size => '200x200',class:"img-responsive"), photo%>
        </div>
      <% end %>
    </div>
  <% end %>
  </div>
