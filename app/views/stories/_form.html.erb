<div class="container text-center">
  <div class="col-md-4">
  </div>
    <%= form_for(@story) do |f| %>
      <% if @story.errors.any? %>
        <div id="error_explanation">
          <h2><%= pluralize(@story.errors.count, "error") %> prohibited this story from being saved:</h2>

          <ul>
          <% @story.errors.full_messages.each do |message| %>
            <li><%= message %></li>
          <% end %>
          </ul>
        </div>
      <% end %>
    <div class="col-md-4">
      <div class="field">
        <%= f.label :Poster %><br>
        <%= f.text_field :title %>
      </div>
      <div class="field">
        <%= f.label :description %><br>
        <%= f.text_area :description %>
      </div>
      <div class="actions">
        <%= f.submit "Submit Post"%>
      </div>
    <% end %>
</div>
<div class="col-md-4">
</div>
</div>
