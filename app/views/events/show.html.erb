
<div id="map-canvas"></div>
<div class="container-fluid" id="main">
  <div class="row">
  	<div class="col-md-8" id="left">
      <h2><%= @event.name %></h2>

      <div>
        <div><h4 class="text-primary">When:</h4></div>
      </div>
          <p>
            <%= @event.date %>
          </p>

          <hr>

      <div>
        <div><h4 class="text-primary">What:</h4></div>
      </div>
          <p>
            <%= @event.description %>
          </p>

          <hr>

      <div>
        <div><h4 class="text-primary">Where:</h4></div>
      </div>
          <p><%= @event.address %></p>
          <p><%= @event.city %>  <%= @event.state %>, <%= @event.zipcode %></p>

          <hr>
      </div>
      <div class="col-md-4"><% my_request = "https://www.google.com/maps/embed/v1/place?key=AIzaSyAIAW3z8RyJAAq5KFcziTTMSk9fh42xyRc&q=#{@event.address}#{@event.city},#{@event.state}#{@event.zipcode}" %>
          <%= content_tag(:iframe, '', src: my_request, width: 400, height: 500, frameborder: 1) %>
      </div>
    </div>
  </div>
<h4><%= link_to 'Edit', edit_event_path(@event) %> |
<%= link_to 'Back', events_path %></h4>
